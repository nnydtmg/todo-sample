{
  "name": "todo-infra",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "test:update": "jest --updateSnapshot",
    "cdk": "cdk",
    "cdk:deploy": "cdk deploy --all",
    "cdk:synth": "cdk synth",
    "cdk:diff": "cdk diff",
    "cdk:nag": "cdk synth --no-path-metadata > template.yaml && npx cfn-nag template.yaml"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.217.0",
    "cdk-docker-image-deployment": "^0.0.909",
    "constructs": "^10.1.115",
    "source-map-support": "^0.5.21"
  },
  "devDependencies": {
    "@types/jest": "^29.5.1",
    "@types/node": "^20.1.0",
    "aws-cdk": "^2.1029.3",
    "cdk-nag": "^2.27.1",
    "jest": "^29.5.0",
    "jest-junit": "^16.0.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "~5.0.4"
  }
}
